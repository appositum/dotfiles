{
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.fontFamily": "'Mononoki Nerd Font', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.automationProfile.windows": {

    },
    "terminal.integrated.defaultProfile.windows": "Debian (WSL)",
    "vim.camelCaseMotion.enable": true,
    "vim.smartRelativeLine": true,
    "vim.neovimUseConfigFile": true,
    "terminal.integrated.rightClickBehavior": "default",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.defaultProfile.linux": "fish",
    "rust-analyzer.cargo.buildScripts.enable": false,
    "rust-analyzer.cargo.buildScripts.useRustcWrapper": false,
    "rust-analyzer.checkOnSave.allTargets": false,
    "workbench.startupEditor": "none",
    "python.languageServer": "None",
    "python.terminal.activateEnvironment": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.testing.promptToConfigure": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "elixirLS.dialyzerEnabled": false,
    //"vim.statusBarColors.insert": "#82b1ff",
    //"vim.statusBarColors.normal": "#7E57C2",
    //"vim.statusBarColors.visual": "#89DDFF",
    //"vim.statusBarColors.visualline": "#ffcb6b",
    //"vim.statusBarColors.visualblock": "#F78C6C",
    //"vim.statusBarColors.replace": "#C3E88D",
    //"workbench.colorCustomizations": {
    //  "statusBar.background": "#005f5f",
    //  "statusBar.noFolderBackground": "#005f5f",
    //  "statusBar.debuggingBackground": "#005f5f",
    //  "statusBar.foreground": "#000000",
    //  "statusBarItem.remoteForeground": "#000000",
    //  "statusBarItem.hoverBackground": "#ffffff4A",
    //  "statusBarItem.activeBackground": "#ffffff80"
    //},
    "vim.normalModeKeyBindingsNonRecursive": [
      {"before": ["w"], "after": ["\\", "w"]},
      {"before": ["b"], "after": ["\\", "b"]},
      {"before": ["e"], "after": ["\\", "e"]},
    ],
    "vim.operatorPendingModeKeyBindingsNonRecursive": [
      {"before": ["i","w"], "after": ["i", "\\", "w"]},
    ],
    "editor.minimap.renderCharacters": false,
    "security.allowedUNCHosts": [
      "wsl.localhost"
    ],
    "ocaml.server.extraEnv": null,
    "git.openRepositoryInParentFolders": "never",
    "python.defaultInterpreterPath": "C:\\Users\\[user]\\AppData\\Local\\Programs\\Python\\Python311\\python.exe",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "workbench.editor.empty.hint": "hidden",
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "window.zoomLevel": 1,
}
